{
    "name": "xandrw/architecture-enforcer",
    "description": "Console command that checks your project's structure against an architecture config",
    "keywords": [
        "php",
        "architecture",
        "structure"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1.0",
        "symfony/console": "^6.4",
        "symfony/finder": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Xandrw\\ArchitectureEnforcer\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Xandrw\\ArchitectureEnforcer\\Tests\\": "tests/"
        }
    },
    "bin": ["enforcer"]
}